          </mesh>
        </geometry>
          <surface>
            <friction>
              <ode>
                <mu>0.5</mu>
                <mu2>1.0</mu2>
                <fdir1>0 0 1</fdir1>
              </ode>
            </friction>
          </surface>
        </collision>
      </link>

      <link name='rear_right_wheel'>
        <pose>0 0 0 0 0 0</pose>
        <!-- <inertial auto="true" /> -->
        <inertial>
          <pose>0.045 0.6 .25 0 0 0</pose>
          <mass>20</mass>
          <inertia>
            <ixx>1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>1</izz>
          </inertia>
        </inertial>
        <visual name='visual'>
          <geometry>
          <mesh>
            <scale>.01 .01 .01</scale>
            <uri>wheel4_1.stl</uri>
          </mesh>
        </geometry>
          <material>
            <ambient>0.2 0.2 0.2 1</ambient>
            <diffuse>0.2 0.2 0.2 1</diffuse>
            <specular>0.2 0.2 0.2 1</specular>
          </material>
        </visual>
        <collision name='collision'>
          <!-- <density>3405.18</density> -->
          <geometry>
          <mesh>
            <scale>.01 .01 .01</scale>
            <uri>wheel4_1.stl</uri>
          </mesh>
        </geometry>
          <surface>
