                <mu2>1.0</mu2>
                <fdir1>0 0 1</fdir1>
              </ode>
            </friction>
          </surface>
        </collision>
      </link>

      <link name="front_left_wheel_steering_link">
        <pose>0 0 0 0 0 0</pose>
        <!-- <inertial auto="true" /> -->
        <inertial>
          <pose>-1.135 -0.065 0.25 0 0 0</pose>
          <mass>5</mass>
          <inertia>
            <ixx>1</ixx>
            <iyy>1</iyy>
            <izz>1</izz>
          </inertia>
        </inertial>
        <visual name="steering_link_visual">
          <pose>0 0 0 0 0 0</pose>
          <geometry>
            <mesh>
              <scale>.01 .01 .01</scale>
              <uri>wheel1_link_1.stl</uri>
            </mesh>
          </geometry>
          <material>
            <ambient>1 1 1</ambient>
            <diffuse>1 1 1</diffuse>
          </material>
        </visual>
        <!-- <collision>
          <density>7850</density>
          <geometry>
            <mesh>
              <scale>.01 .01 .01</scale>
              <uri>wheel1_link_1.stl</uri>
            </mesh>
          </geometry>
        </collision> -->

      </link>

      <link name="front_right_wheel_steering_link">
        <pose>0 0 0 0 0 0</pose>
        <!-- <inertial auto="true" /> -->
        <inertial>
          <pose>-1.135 0.51 0.25 0 0 0</pose>
          <mass>5</mass>
          <inertia>
            <ixx>1</ixx>
            <iyy>1</iyy>
            <izz>1</izz>
